{
    "name": "async-aws/cloud-formation",
    "type": "library",
    "description": "Async AWS CloudFormation client.",
    "keywords": [
        "aws",
        "lambda"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-SimpleXML": "*",
        "ext-filter": "*",
        "async-aws/core": "^0.3",
        "symfony/http-client-contracts": "^1.0 || ^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\CloudFormation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\CloudFormation\\Tests\\": "tests/"
        }
    }
}
