{
    "name": "async-aws/core",
    "type": "library",
    "description": "AsyncAws core package with common classes and interfaces.",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "sts"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-SimpleXML": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "psr/log": "~1.0",
        "symfony/http-client": "^4.4 || ^5.0",
        "symfony/http-client-contracts": "^1.1.8 || ^2.0",
        "symfony/service-contracts": "^1.0 || ^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Core\\Tests\\": "tests/"
        }
    }
}
