{
    "name": "async-aws/async-aws",
    "type": "library",
    "description": "Root package, that include all AWS API clients",
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-SimpleXML": "*",
        "ext-hash": "*",
        "psr/log": "~1.0",
        "symfony/http-client": "^4.4 || ^5.0",
        "symfony/http-client-contracts": "^1.0 || ^2.0",
        "symfony/service-contracts": "^2.0"
    },
    "require-dev": {
        "ext-json": "*",
        "nette/php-generator": "^3.3",
        "symfony/console": "^5.0",
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\": "src",
            "AsyncAws\\Build\\": "build"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Test\\": "tests"
        }
    }
}
