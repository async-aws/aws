{
    "name": "async-aws/async-aws",
    "type": "library",
    "description": "Root package, that include all AWS API clients",
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-SimpleXML": "*",
        "ext-dom": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "psr/log": "~1.0",
        "symfony/http-client": "^4.4 || ^5.0",
        "symfony/http-client-contracts": "^1.0 || ^2.0",
        "symfony/process": "^4.4 || ^5.0",
        "symfony/service-contracts": "^1.0 || ^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "league/flysystem": "^2.0@dev",
        "matthiasnoback/symfony-config-test": "^4.1",
        "nette/php-generator": "^3.3",
        "nette/utils": "^3.0",
        "nyholm/symfony-bundle-test": "^1.6.1",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/console": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\CloudFormation\\": "src/CloudFormation/src",
            "AsyncAws\\CodeGenerator\\": "src/CodeGenerator/src",
            "AsyncAws\\Core\\": "src/Core/src",
            "AsyncAws\\Flysystem\\S3\\": "src/Flysystem/S3/src",
            "AsyncAws\\Lambda\\": "src/Lambda/src",
            "AsyncAws\\S3\\": "src/S3/src",
            "AsyncAws\\Ses\\": "src/Ses/src",
            "AsyncAws\\Sns\\": "src/Sns/src",
            "AsyncAws\\Sqs\\": "src/Sqs/src",
            "AsyncAws\\Symfony\\Bundle\\": "src/Symfony/Bundle/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Test\\": "tests",
            "AsyncAws\\CloudFormation\\Tests\\": "src/CloudFormation/tests",
            "AsyncAws\\CodeGenerator\\Tests\\": "src/CodeGenerator/tests",
            "AsyncAws\\Core\\Tests\\": "src/Core/tests",
            "AsyncAws\\Flysystem\\S3\\Tests\\": "src/Flysystem/S3/tests",
            "AsyncAws\\Lambda\\Tests\\": "src/Lambda/tests",
            "AsyncAws\\S3\\Tests\\": "src/S3/tests",
            "AsyncAws\\Ses\\Tests\\": "src/Ses/tests",
            "AsyncAws\\Sns\\Tests\\": "src/Sns/tests",
            "AsyncAws\\Sqs\\Tests\\": "src/Sqs/tests",
            "AsyncAws\\Symfony\\Bundle\\Tests\\": "src/Symfony/Bundle/tests"
        }
    }
}
