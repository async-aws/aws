{
    "name": "async-aws/monolog-cloud-watch",
    "description": "Monolog integration with CloudWatch logs",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aws",
        "aync-aws",
        "Monolog",
        "CloudWatch",
        "Logs"
    ],
    "require": {
        "php": "^8.2",
        "async-aws/cloud-watch-logs": "^1.0 || ^2.0",
        "monolog/monolog": "^1.1 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.42",
        "symfony/error-handler": "^7.3.2 || ^8.0",
        "symfony/phpunit-bridge": "^7.3.2 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Monolog\\CloudWatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Monolog\\CloudWatch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
