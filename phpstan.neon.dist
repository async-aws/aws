includes:
    - phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - src

    excludes_analyse:
        - src/*/Tests/*
        - src/**/Tests/*
        - src/CodeGenerator/Generator/PhpGenerator/ClassFactory.php

    ignoreErrors:
        - '|Ternary operator condition is always true\.|'
        - '|Negated boolean expression is always false\.|'
