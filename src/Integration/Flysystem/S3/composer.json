{
    "name": "async-aws/flysystem-s3",
    "type": "library",
    "description": "Flysystem S3 adapter from AsyncAws.",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "s3",
        "Flysystem",
        "Filesystem"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "async-aws/s3": "^1.5",
        "league/flysystem": "^1.0.65",
        "league/mime-type-detection": "^1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2 || ^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Flysystem\\S3\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Flysystem\\S3\\Tests\\": "tests/"
        }
    }
}
