{
    "name": "async-aws/flysystem-s3",
    "type": "library",
    "description": "Flysystem S3 adapter from AsyncAws.",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "s3",
        "Flysystem",
        "Filesystem"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "async-aws/core": "^1.0",
        "async-aws/s3": "^1.0",
        "league/flysystem": "^1.0.65 || ^2.0.0-alpha.2",
        "league/mime-type-detection": "^1.0"
    },
    "require-dev": {
        "league/flysystem-adapter-test-utilities": "2.x-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Flysystem\\S3\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Flysystem\\S3\\Tests\\": "tests/"
        }
    }
}
