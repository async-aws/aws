{
    "name": "async-aws/iot",
    "description": "Iot client, part of the AWS SDK provided by AsyncAws.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "iot"
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-filter": "*",
        "ext-json": "*",
        "async-aws/core": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Iot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Iot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
