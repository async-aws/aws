{
    "name": "async-aws/cloud-formation",
    "description": "CloudFormation client, part of the AWS SDK provided by AsyncAws.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "cloud-formation"
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-SimpleXML": "*",
        "ext-filter": "*",
        "async-aws/core": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\CloudFormation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\CloudFormation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
