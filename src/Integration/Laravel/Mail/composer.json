{
    "name": "async-aws/illuminate-mail",
    "description": "AsyncAws integration for Illuminate Mail.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aws",
        "async-aws",
        "illuminate",
        "ses",
        "mail"
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "async-aws/ses": "^1.0",
        "illuminate/mail": "^6.18.13 || ^7.10 || ^8.0",
        "illuminate/support": "^6.18.13 || ^7.10 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Illuminate\\Mail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Illuminate\\Mail\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        },
        "laravel": {
            "providers": [
                "AsyncAws\\Illuminate\\Mail\\ServiceProvider"
            ]
        }
    }
}
