{
    "name": "async-aws/dynamo-db",
    "type": "library",
    "description": "DynamoDb client, part of the AWS SDK provided by AsyncAws.",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "dynamo-db"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-filter": "*",
        "ext-json": "*",
        "async-aws/core": "^1.9"
    },
    "conflict": {
        "symfony/http-client": "<4.4.16,<5.1.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\DynamoDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\DynamoDb\\Tests\\": "tests/"
        }
    }
}
