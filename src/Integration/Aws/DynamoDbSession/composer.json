{
    "name": "async-aws/dynamo-db-session",
    "description": "AsyncAws DynamoDB integration for PHP Sessions.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "dynamo-db",
        "session"
    ],
    "require": {
        "php": "^8.2",
        "async-aws/dynamo-db": "^1.0 || ^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\DynamoDbSession\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\DynamoDbSession\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
