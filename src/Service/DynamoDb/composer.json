{
    "name": "async-aws/dynamo-db",
    "description": "DynamoDb client, part of the AWS SDK provided by AsyncAws.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "dynamo-db"
    ],
    "require": {
        "php": "^8.2",
        "ext-filter": "*",
        "ext-json": "*",
        "async-aws/core": "^1.16",
        "symfony/polyfill-uuid": "^1.13.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.42",
        "symfony/error-handler": "^7.3.2 || ^8.0",
        "symfony/phpunit-bridge": "^7.3.2 || ^8.0"
    },
    "conflict": {
        "symfony/http-client": "<4.4.16 <5.1.7"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\DynamoDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\DynamoDb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.9-dev"
        }
    }
}
