    "require": {
        "php": "^7.2.5",
        "ext-SimpleXML": "*",
        "ext-hash": "*",
        "nette/php-generator": "^3.3",
        "symfony/console": "^5.0",
        "symfony/http-client": "^4.4 || ^5.0",
