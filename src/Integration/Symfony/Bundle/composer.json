{
    "name": "async-aws/async-aws-bundle",
    "type": "symfony-bundle",
    "description": "Configure all your AsyncAws services and enjoy the autowire greatness.",
    "keywords": [
        "aws",
        "amazon",
        "sdk",
        "async-aws",
        "symfony",
        "bundle"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5 || ^8.0",
        "async-aws/core": "^1.0",
        "symfony/config": "^4.4 || ^5.0 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.0 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "async-aws/s3": "^1.0",
        "async-aws/ses": "^1.0",
        "async-aws/sqs": "^1.0",
        "async-aws/ssm": "^1.0",
        "matthiasnoback/symfony-config-test": "^4.1",
        "nyholm/symfony-bundle-test": "^1.6.1",
        "symfony/cache": "^4.4 || ^5.0 || ^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Symfony\\Bundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Symfony\\Bundle\\Tests\\": "tests/"
        }
    }
}
