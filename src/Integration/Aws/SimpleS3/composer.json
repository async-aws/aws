{
    "name": "async-aws/simple-s3",
    "description": "A simple S3 client that are easy to work with. ",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aws",
        "async-aws",
        "amazon",
        "sdk",
        "s3"
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "async-aws/s3": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.42",
        "symfony/error-handler": "^7.3.2 || ^8.0",
        "symfony/phpunit-bridge": "^7.3.2 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\SimpleS3\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\SimpleS3\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
