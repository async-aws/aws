{
    "name": "async-aws/illuminate-filesystem",
    "description": "AsyncAws integration for Illuminate Filesystem.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aws",
        "async-aws",
        "illuminate",
        "filesystem"
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "async-aws/core": "^1.0",
        "async-aws/flysystem-s3": "^1.0",
        "async-aws/simple-s3": "^1.0",
        "illuminate/filesystem": "^6.18.13 || ^7.10 || ^8.0",
        "illuminate/support": "^6.18.13 || ^7.10 || ^8.0"
    },
    "conflict": {
        "league/flysystem": ">=2.0.0"
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Illuminate\\Filesystem\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Illuminate\\Filesystem\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        },
        "laravel": {
            "providers": [
                "AsyncAws\\Illuminate\\Filesystem\\ServiceProvider"
            ]
        }
    }
}
