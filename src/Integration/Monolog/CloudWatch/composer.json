{
    "name": "async-aws/monolog-cloud-watch",
    "type": "library",
    "description": "Monolog integration with CloudWatch logs",
    "keywords": [
        "aws",
        "aync-aws",
        "Monolog",
        "CloudWatch",
        "Logs"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5 || ^8.0",
        "async-aws/cloud-watch-logs": "^0.1",
        "monolog/monolog": "^1.1 || ^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "AsyncAws\\Monolog\\CloudWatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncAws\\Monolog\\CloudWatch\\Tests\\": "tests/"
        }
    }
}
